<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_Match_Account__c</fullName>
    <description>Field used to check if the contact has the same phone number of the related account</description>
    <externalId>false</externalId>
    <formula>IF(Phone = Account.Phone, true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Phone Match Account</label>
    <type>Checkbox</type>
</CustomField>
